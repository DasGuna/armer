<launch>
    <group ns="arm2">
    <arg name="backend" default="manipulation_driver.backends.ROS" />
    <arg name="robot" default="roboticstoolbox.models.Panda" />

    <node pkg="manipulation_driver" type="driver" name="driver" output="screen">
        <param name="backend" value="$(arg backend)" />
        <param name="robot" value="$(arg robot)" />
    </node>
    </group>
</launch>