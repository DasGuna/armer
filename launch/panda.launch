<launch>
    <arg name="backend" default="roboticstoolbox.backends.Swift" />
    <arg name="robot" default="roboticstoolbox.models.Panda" />

    <node pkg="manipulation_driver" type="driver" name="driver">
        <param name="backend" value="$(arg backend)" />
        <param name="robot" value="$(arg robot)" />
    </node>
</launch>